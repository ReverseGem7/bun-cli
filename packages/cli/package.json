{
	"name": "clivex",
	"version": "0.2.2",
	"description": "A CLI tool for development",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"type": "module",
	"scripts": {
		"build": "tsdown",
		"check-types": "tsc --noEmit",
		"publish": "bun publish"
	},
	"keywords": [
		"cli",
		"development",
		"tool"
	],
	"author": "ReverseGem7",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ReverseGem7/clivex.git"
	},
	"bugs": {
		"url": "https://github.com/ReverseGem7/clivex/issues"
	},
	"homepage": "https://github.com/ReverseGem7/clivex#readme",
	"devDependencies": {
		"@types/bun": "latest",
		"biome": "^0.3.3",
		"tsdown": "^0.13.3"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
